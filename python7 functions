import builtins
# get a list of all built-in names
builtin_names =dir(builtins)
print("all bulit-in names:")
print(builtin_names)



4ans

def create_product(price,name="laptop"):
     return {"name":name, "price":price}
product1= create_product("laptop",1200.00)
product2= create_product(1200.00)
print(product1)
print(product2)




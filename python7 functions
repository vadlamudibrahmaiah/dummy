# functions
numbers=[1,2,3,4,5,6]
for i in numbers:
    print(i**2)
    print(i**3)


2ans

def function1(numbers,x):
  for i in numbers:
    print(i**x)

xyz=(function1([1,2,3,4,5,6],7))
print(xyz)



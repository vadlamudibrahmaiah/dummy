num1 = 20
num2 = 23
operator = "*"
result=num1 * num2
if operator == "+":
   print(result)
elif operator == "-":
   print(result)
elif operator == "*":
   print(result)
elif operator == "/":
   print(result)
else:
   print("wrong operator")





def convert_float(a):
  print(type(a))
  print(int(a))
  print(str(a))
  print(bool(a))
  # print(list(a))
  print(list(str(a)))
  print(tuple(str(a)))
  print(dict,{1:a})
  print(set(str(a)))
  print(complex(a))


a=5.5
convert_float(a)




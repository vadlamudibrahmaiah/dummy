def convert_dict(a):
  print(type(a))
  # print(float(a))
  z=str(a)
  print(type(z))
  print("String",str(a))

  print(bool(a))
  print(list(a))
  print(tuple(a))
  print(set(a))
  print(dict({1:a}))
  # print(complex(a))

a = {"name": "Alice", "age": 30, "city": "NewÂ York"}
convert_dict(a)



8ans

def convert_int(x, to_type):
    if to_type == "int":
        return int(x)
    elif to_type == "float":
        return float(x)
    elif to_type == "str":
        return str(x)
    elif to_type == "bool":
        return bool(x)
    elif to_type == "list":
        return list(str(x))
    elif to_type == "tuple":
        return tuple(str(x))
    elif to_type == "dict":
        return {1: x}
    elif to_type == "set":
        return set(str(x))
    elif to_type == "complex":
        return complex(x)
    else:
        return "Unsupported type"

# Example usage:
x = 5
print(convert_int(x,"float"))   # 5.0
print(convert_int(x, "list"))    # ['5']
print(convert_int(x, "complex")) # (5+0j)







































































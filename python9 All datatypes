def convert_int(x):
  print(type(x))
  print(float(x))
  print(str(x))
  print(bool(x))
  # print(list(x))
  print(list(str(x)))
  print(tuple(str(x)))
  print("DICT",dict({1:x}))
  print(set(str(x)))
  print(complex(x))

x=5
convert_int(x)



